@model CafeManagement.Models.Store

<h2>Chi tiết cửa hàng</h2>

<div>
    <h4>Cửa hàng</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Tên
        </dt>
        <dd class="col-sm-10">
            @Model.StoreName
        </dd>
        <dt class="col-sm-2">
            Địa chỉ
        </dt>
        <dd class="col-sm-10">
            @Model.Address
        </dd>
        <dt class="col-sm-2">
            Số điện thoại
        </dt>
        <dd class="col-sm-10">
            @Model.Phone
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.StoreId">Edit</a> |
    <a asp-action="Index">Quay về danh sách</a>
</div>